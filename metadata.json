{
  "name": "puppet-augeasproviders_base",
  "version": "2.1.0",
  "author": "Raphael Pinson",
  "summary": "Augeas-based base providers for Puppet",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-augeasproviders_base",
  "project_page": "http://augeasproviders.com",
  "issues_url": "https://github.com/voxpupuli/puppet-augeasproviders_base/issues",
  "description": "This module provides providers for Puppet base types using the Augeas configuration API library.",
  "dependencies": [
    {
      "name": "voxpupuli/augeasproviders_core",
      "version_requirement": ">=3.0.0 < 4.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7",
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "16.04",
        "18.04",
        "18.10"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 6.1.0 < 8.0.0"
    }
  ]
}
